<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false" package="com.srts.examination.domain">
 <class name="TestInfo" table="exm_testInfo">
  <id name="id">
   <generator class="native"/>
  </id>
  <property  name="testName"/>
  <property  name="testDate"/>
  <property  name="testTime"/>
   <property name="endTime"/>
   <property name="testTimeLength"/>
  <property name="testPlace"/>
  <property name="testContent"/>
  <property  name="testCompany"/>
  <property  name="testPeopleNum"/>
  <property  name="testPaperProCondition"/>
  <property  name="testPaperEvaCondition"/>
  <property  name="testPaperCondition"/>
  <property name="testGradeCondition"/>
   <property name="state"/>
 </class>
 

</hibernate-mapping>
