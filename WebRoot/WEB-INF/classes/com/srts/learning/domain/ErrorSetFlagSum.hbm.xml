<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="com.srts.learning.domain">	
	<class name="ErrorSetFlagSum" table="lrn_errorSetFlagSum">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="flagSum"/>
		<property name="TimePoint"/>
		<many-to-one name="usr" class="com.srts.system.domain.Sys_User" column="usrId"/>
	</class>
</hibernate-mapping>



