<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="com.srts.knowledge.domain">	
	<class name="TypicalCase" table="klg_typicalCase">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="title"/>
		<property name="searchnum" type="java.lang.Long" />
		<property name="content" type="text"/>
		<property name="uploaddate"/>
		<property name="updatedate" type="java.lang.String"/>
		<property name="type" type="java.lang.String"/>
		<property name="pic"  type="java.lang.String"></property>
		
	</class>
</hibernate-mapping>



