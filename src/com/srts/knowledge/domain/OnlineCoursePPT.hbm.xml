<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="com.srts.knowledge.domain">
	<class name="OnlineCoursePPT" table="klg_courseppt">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="pptName"/>
		<property name="pptIntro"/>
		<property name="saveURL"/>
		<property name="pptImgURL"/>
		<property name="uploadDate"/>
		<property name="readCount"/>
		<property name="loadCount"/>
		<property name="uploadUsr"/>
	</class>
</hibernate-mapping>



