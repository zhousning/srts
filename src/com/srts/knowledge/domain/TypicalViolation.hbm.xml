<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="com.srts.knowledge.domain">	
	<class name="TypicalViolation" table="klg_typicalViolation" >
		<id name="id">
			<generator class="native" />
		</id>
		<property name="title"  type="java.lang.String" />
		<property name="searchnum" type="java.lang.Long"/>
		<property name="content" />
		<property name="uploaddate" type="java.lang.String"/>
		<property name="updatedate" type="java.lang.String"/>
		<property name="type"/>
	</class>
</hibernate-mapping>



