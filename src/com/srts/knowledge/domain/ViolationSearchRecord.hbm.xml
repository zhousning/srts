<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="com.srts.knowledge.domain">	
	<class name="ViolationSearchRecord" table="klg_violationSearchRecord">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="content_id"/>
		<property name="searchdate"/>
		<many-to-one name="user" class="com.srts.system.domain.Sys_User" column="usrId"/>
	</class>
</hibernate-mapping>



